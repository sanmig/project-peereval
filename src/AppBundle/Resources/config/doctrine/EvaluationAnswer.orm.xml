<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\EvaluationAnswer" table="evaluation_answer">
    <indexes>
      <index name="form" columns="form"/>
      <index name="student" columns="student"/>
      <index name="question" columns="question"/>
      <index name="answer" columns="answer"/>
    </indexes>
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="status" type="smallint" column="status" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="formcode" type="string" column="formCode" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="feedback" type="text" column="feedback" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="attemptat" type="datetime" column="attemptAt" nullable="true"/>
    <many-to-one field="form" target-entity="EvaluationForm" fetch="LAZY">
      <join-columns>
        <join-column name="form" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="student" target-entity="StudentMain" fetch="LAZY">
      <join-columns>
        <join-column name="student" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="question" target-entity="QuestionMain" fetch="LAZY">
      <join-columns>
        <join-column name="question" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="answer" target-entity="AnswerMain" fetch="LAZY">
      <join-columns>
        <join-column name="answer" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\EvaluationAnswer" table="evaluation_answer">
    <indexes>
      <index name="formId" columns="formId"/>
      <index name="studentId" columns="studentId"/>
      <index name="questionId" columns="questionId"/>
    </indexes>
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="answer" type="smallint" column="answer" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="status" type="smallint" column="status" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="formcode" type="string" column="formCode" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="feedback" type="text" column="feedback" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="attemptat" type="datetime" column="attemptAt" nullable="true"/>
    <many-to-one field="formid" target-entity="EvaluationForm" fetch="LAZY">
      <join-columns>
        <join-column name="formId" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="studentid" target-entity="StudentMain" fetch="LAZY">
      <join-columns>
        <join-column name="studentId" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="questionid" target-entity="QuestionMain" fetch="LAZY">
      <join-columns>
        <join-column name="questionId" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

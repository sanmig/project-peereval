<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\StudentMain" table="student_main">
    <unique-constraints>
      <unique-constraint name="weltecId" columns="weltecId"/>
    </unique-constraints>
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="weltecid" type="bigint" column="weltecId" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="firstname" type="string" column="firstName" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastname" type="string" column="lastName" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="expiryat" type="datetime" column="expiryAt" nullable="true"/>
  </entity>
</doctrine-mapping>
